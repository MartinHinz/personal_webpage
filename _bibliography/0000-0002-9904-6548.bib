@misc{martin_oxcaar:_2018, title= {{oxcAAR}: {Interface} to '{OxCal}' {Radiocarbon} {Calibration}}, copyright= {GPL-2 {\textbar} file LICENSE}, shorttitle= {{oxcAAR}}, url= {https://CRAN.R-project.org/package=oxcAAR}, abstract= {A set of tools that enables using 'OxCal' from within R. 'OxCal' ({\textless}https://c14.arch.ox.ac.uk/oxcal.html{\textgreater}) is a standard archaeological tool intended to provide 14C calibration and analysis of archaeological and environmental chronological information. 'OxcAAR' allows simple calibration with 'Oxcal' and plotting of the results as well as the execution of sophisticated ('OxCal') code and the import of the results of bulk analysis and complex Bayesian sequential calibration.}, urldate= {2019-01-15}, author= {Hinz, Martin and Schmid, Clemens and Knitter, Daniel and Tietze, Carolin}, month= {feb}, year= {2018}}



