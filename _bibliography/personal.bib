@article{hinz_absolute_2015,
	title = {The {Absolute} {Speed} of {Change}: {Multidimensional} {Scaling} and {Innovation} {Rates}},
	volume = {57},
	copyright = {© 2014 University of Oxford},
	issn = {1475-4754},
	shorttitle = {The {Absolute} {Speed} of {Change}},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/arcm.12111/abstract},
	doi = {10.1111/arcm.12111},
	abstract = {The evaluation of societal developments depends on a clear determination of the rate of change that occurs within societies. While many statistical approaches are able to indicate similarity/dissimilarity distances between archaeological assemblages, only a few can be linked to linear timescales. In the following approach, the advantages of non-metric multidimensional scaling (NMDS) are described as a method that produces gradients for a quantitative measurement of cultural change on a linear timescale. Thus, the speed of innovations or the regression of cultural developments could be quantified. In consequence, a quantified proxy of societal developments on a linear timescale is provided. In two examples, the usefulness of such a proxy is described in relation to other data.},
	language = {en},
	number = {3},
	urldate = {2015-09-24},
	journal = {Archaeometry},
	author = {Hinz, M. and Müller, J.},
	month = jun,
	year = {2015},
	keywords = {14C, Bronze Age, Calibration, Correspondence Analysis, Cultural Development, eigene, Innovation, Neolithic, Non-metric Multidimensional Scaling, Typology},
	pages = {560--581}
}

